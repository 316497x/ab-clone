{
  "name": "ab-clone",
  "version": "1.0.0",
  "main": "main.js",
  "description": "AimBooster Clone",
  "author": "YourName",
  "license": "MIT",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "devDependencies": {
    "electron": "^36.4.0",
    "electron-builder": "^26.0.12"
  },
  "build": {
    "appId": "com.yourname.abclone",
    "productName": "AimBoosterClone",
    "files": [
      "**/*",
      "!node_modules/*/{test,tests,__tests__}/**",
      "!**/*.md",
      "!**/dist-temp"
    ],
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    }
  }
}